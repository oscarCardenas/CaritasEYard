<h1>Listing workshop_course_types</h1>

<table>
  <tr>
    <th>Id course type</th>
    <th>Id workshop</th>
  </tr>

<% @workshop_course_types.each do |workshop_course_type| %>
  <tr>
    <td><%=h workshop_course_type.id_course_type %></td>
    <td><%=h workshop_course_type.id_workshop %></td>
    <td><%= link_to 'Show', workshop_course_type %></td>
    <td><%= link_to 'Edit', edit_workshop_course_type_path(workshop_course_type) %></td>
    <td><%= link_to 'Destroy', workshop_course_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New workshop_course_type', new_workshop_course_type_path %>