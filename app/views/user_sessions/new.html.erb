<h1>Login</h1>

<% form_for(@user_session) do |f| %>
  <%= f.error_messages %>

    <table>
    <tr>
      <td>*Nombre de usuario</td>
      <td><%= f.text_field :username %></td>
    </tr>
    <tr>
      <td>*Contrase√±a</td>
      <td><%= f.password_field :password %></td>
    </tr>
    <tr align="right">
      <td colspan="2"><%= f.submit 'Iniciar sesion' %> <%= link_to 'Atras', root_url %></td>
    </tr>
  </table>
<% end %>
